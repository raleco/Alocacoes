    <validationRules>
        <fullName>FinalDateContractPreviousTheInitialDate</fullName>
        <active>true</active>
        <description>The end date of the Agreement cannot be earlier than the start date of the Agreement.</description>
        <errorConditionFormula>OR(
 YEAR(ContractStartDate__c)&gt; YEAR(ContractEndDate__c),
 AND(
  DAY(ContractStartDate__c) &gt; DAY(ContractEndDate__c) ,
  MONTH(ContractStartDate__c)= MONTH(ContractEndDate__c),
  YEAR(ContractStartDate__c)= YEAR(ContractEndDate__c)
 ),
 AND(
  MONTH(ContractStartDate__c)&gt; MONTH(ContractEndDate__c),
  YEAR(ContractStartDate__c)= YEAR(ContractEndDate__c)
 )
)</errorConditionFormula>
        <errorDisplayField>ContractEndDate__c</errorDisplayField>
        <errorMessage>The end date of the Agreement cannot be earlier than the start date of the Agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinalDatePreviousProjectAInitialDate</fullName>
        <active>true</active>
        <description>The expected end or actual end date cannot be less than the Project start date.</description>
        <errorConditionFormula>OR(
 YEAR(EndDate__c)&lt; YEAR(StartDate__c),
 AND(
  DAY(EndDate__c) &lt; DAY(StartDate__c) , 
  MONTH(EndDate__c)= MONTH(StartDate__c),
  YEAR(EndDate__c)= YEAR(StartDate__c)
 ),
 AND( 
  MONTH(EndDate__c)&lt; MONTH(StartDate__c),
  YEAR(EndDate__c)= YEAR(StartDate__c)
 ),
 YEAR(EndDateReal__c)&lt; YEAR(StartDate__c),
 AND(
  DAY(EndDateReal__c) &lt; DAY(StartDate__c) , 
  MONTH(EndDateReal__c)= MONTH(StartDate__c),
  YEAR(EndDateReal__c)= YEAR(StartDate__c)
 ),
 AND( 
  MONTH(EndDateReal__c)&lt; MONTH(StartDate__c),
  YEAR(EndDateReal__c)= YEAR(StartDate__c)
 )
)</errorConditionFormula>
        <errorMessage>The expected end or actual end date cannot be less than the Project start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinalDateStoppingPreviousTheInitialDate</fullName>
        <active>true</active>
        <description>The end date of the Outage cannot be earlier than the start date of the Outage.</description>
        <errorConditionFormula>OR(
 YEAR(StoppageEndDate__c)&lt; YEAR(StoppageDate__c),
 AND(
  DAY(StoppageEndDate__c) &lt; DAY(StoppageDate__c) , 
  MONTH(StoppageEndDate__c)= MONTH(StoppageDate__c),
  YEAR(StoppageEndDate__c)= YEAR(StoppageDate__c)
 ),
 AND( 
  MONTH(StoppageEndDate__c)&lt; MONTH(StoppageDate__c),
  YEAR(StoppageEndDate__c)= YEAR(StoppageDate__c)
 )
)</errorConditionFormula>
        <errorDisplayField>StoppageEndDate__c</errorDisplayField>
        <errorMessage>The end date of the Outage cannot be earlier than the start date of the Outage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RealDate</fullName>
        <active>true</active>
        <description>If the project status is inactive or delivered, the final (actual) date field must be filled in.</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , &quot;Inativo&quot;) || ISPICKVAL( Status__c , &quot;Entregue&quot;),
ISBLANK( EndDateReal__c ))</errorConditionFormula>
        <errorDisplayField>EndDateReal__c</errorDisplayField>
        <errorMessage>Insert End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidaFillingEngagement</fullName>
        <active>true</active>
        <description>Every project must have an Engagement number.</description>
        <errorConditionFormula>ISBLANK( Engagement__c )</errorConditionFormula>
        <errorDisplayField>Engagement__c</errorDisplayField>
        <errorMessage>Please fill in the Engagement field.</errorMessage>
    </validationRules>
