        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Funcao</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>SimilarEmployee__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Relacionamento de pesquisa funcionário semelhante</description>
        <externalId>false</externalId>
        <inlineHelpText>Funcionário similar</inlineHelpText>
        <label>Similar Employee</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Funcionário semelhante não pode ser o mesmo do funcionário de origem.</errorMessage>
            <filterItems>
                <field>Employee__c.Id</field>
                <operation>notEqual</operation>
                <valueField>$Source.SourceEmployee__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Employee__c</referenceTo>
        <relationshipLabel>Similar Employee Profiles (Funcionário Semelhante)</relationshipLabel>
