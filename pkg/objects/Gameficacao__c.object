    <validationRules>
        <fullName>Distribuicao_invalida</fullName>
        <active>true</active>
        <description>Funcionário não pode dar ponto para si mesmo.</description>
        <errorConditionFormula>Pontuado__c = Distribuidor__c</errorConditionFormula>
        <errorDisplayField>Pontuado__c</errorDisplayField>
        <errorMessage>Funcionário não pode distribuir pontos para si mesmo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pontos_excedidos</fullName>
        <active>true</active>
        <description>Pontuação máxima permitida para uma ÚNICA pessoa é de 250 pontos.</description>
        <errorConditionFormula>Ponto__c  &gt; 250</errorConditionFormula>
        <errorDisplayField>Ponto__c</errorDisplayField>
        <errorMessage>Pontuação máxima permitida é de 250 pontos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pontuacao_Distribuida</fullName>
        <active>true</active>
        <errorConditionFormula>Pontuado__r.Pontos_distribuidos__c   &gt;  500</errorConditionFormula>
        <errorDisplayField>Ponto__c</errorDisplayField>
        <errorMessage>Já distribuiu a pontuação máxima.</errorMessage>
